<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".usuarios.ere_covid.ReportarResultados">

    <EditText
        android:id="@+id/tf_reporte_resultados_Cedula"
        android:layout_width="243dp"
        android:layout_height="38dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="@+id/lbl_reporte_resultados_Titulo"
        app:layout_constraintTop_toBottomOf="@+id/lbl_reporte_resultados_Cedula" />

    <TextView
        android:id="@+id/lbl_reporte_resultados_Cedula"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="Cedula"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/lbl_reporte_resultados_Titulo"
        app:layout_constraintTop_toBottomOf="@+id/lbl_reporte_resultados_Titulo" />

    <TextView
        android:id="@+id/lbl_reporte_resultados_Titulo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="130dp"
        android:text="Envio de Resultados"
        android:textSize="36sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_reporte_resultados_Envio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="Enviar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spn_reporte_resultados_Resultado" />

    <Spinner
        android:id="@+id/spn_reporte_resultados_Resultado"
        android:layout_width="317dp"
        android:layout_height="34dp"
        android:layout_marginTop="8dp"
        android:entries="@array/resultado_examen"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lbl_reporte_resultados_Resultado" />

    <TextView
        android:id="@+id/lbl_reporte_resultados_Resultado"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Resultado"
        app:layout_constraintStart_toStartOf="@+id/lbl_reporte_resultados_Cedula"
        app:layout_constraintTop_toBottomOf="@+id/tf_reporte_resultados_Cedula" />

    <Spinner
        android:id="@+id/spn_reporte_resultados_TipoID"
        android:layout_width="68dp"
        android:layout_height="37dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:entries="@array/Cedula"
        app:layout_constraintEnd_toStartOf="@+id/tf_reporte_resultados_Cedula"
        app:layout_constraintStart_toStartOf="@+id/lbl_reporte_resultados_Cedula"
        app:layout_constraintTop_toBottomOf="@+id/lbl_reporte_resultados_Cedula" />
</androidx.constraintlayout.widget.ConstraintLayout>